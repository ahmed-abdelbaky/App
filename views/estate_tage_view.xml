<odoo>
    <data>

        <record id="estate_tage_action" model="ir.actions.act_window">
            <field name="name">Estate Tag Action</field>
            <field name="res_model">estate.property.tag</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="estate_tag_list" model="ir.ui.view">
            <field name="name">Estate Tag List</field>
            <field name="model">estate.property.tag</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="estate_tag_form" model="ir.ui.view">
            <field name="name">Estate Tag Form</field>
            <field name="model">estate.property.tag</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>